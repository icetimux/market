<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Welcome Shop - {% block title %}{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<h1>Shop</h1>
<p>
    Cart: 0 |
    <a href="">Checkout</a>
</p>
<div class="ui divider"></div>
<div class="force-inline">
    <a href="{{ path('app_shop') }}">Home</a> |
    <a href="{{ path('app_shop_products') }}">Products</a>
</div>
<div class="ui divider"></div>
{% block body %}{% endblock %}
</body>
</html>
